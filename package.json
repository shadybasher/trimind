    "svix": "^1.81.0"
  },
  "devDependencies": {
    "@clerk/testing": "^1.13.14",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.7.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
